    <gazebo>
        <plugin filename="libgazebo_grasp_fix.so" name="gazebo_grasp_fix">
            <arm>
                <arm_name>my_gripper</arm_name>
                <palm_link>wrist_3_link</palm_link>
                <!--<palm_link>robot_arm_tool0</palm_link>-->
                <gripper_link>egh_gripper_left_finger</gripper_link>
                <gripper_link>egh_gripper_right_finger</gripper_link>
            </arm>
            <forces_angle_tolerance>100</forces_angle_tolerance>
            <update_rate>10</update_rate>
            <grip_count_threshold>3</grip_count_threshold>
            <max_grip_count>10</max_grip_count>
            <release_tolerance>0.001</release_tolerance>
            <disable_collisions_on_attach>false</disable_collisions_on_attach>
            <contact_topic>__default_topic__</contact_topic>
        </plugin>
    </gazebo>
    <!-- camera body, with origin at bottom screw mount -->
    <joint name="camera_rgbd_camera_joint" type="fixed">
        <!--<xacro:insert_block name="origin" />-->
        <!--<origin xyz="1.0 0.1 0.1" rpy="0 ${pi/6} ${-pi}"/>-->
        <!--origin xyz="0.5 0.75 0.1" rpy="0 ${pi/6} ${-pi/2}"/-->
        <origin rpy="0 1.5707963267948966 0" xyz="1.0 0.0 1.5" />
        <parent link="world" />
        <child link="camera_rgbd_camera_link" />
    </joint>
    <link name="camera_rgbd_camera_link">
        <visual>
            <!-- origin xyz="0 ${-r430_cam_mount_from_center_offset} ${r430_cam_height/2}" rpy="${M_PI/2} 0 ${M_PI/2}"/ -->
            <origin rpy="0 0 0" xyz="0 -0.0 0" />
            <geometry>
                <!-- box size="${r430_cam_width} ${r430_cam_height} ${r430_cam_depth}"/ -->
                <mesh filename="package://robotnik_sensors/meshes/intel_r430.dae" />
            </geometry>
            <material name="aluminum">
                <color rgba="0.5 0.5 0.5 1" />
            </material>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="-0.0125 0.0 0.0" />
            <geometry>
                <box size="0.025 0.1 0.025" />
            </geometry>
        </collision>
        <inertial>
            <!-- The following are not reliable values, and should not be used for modeling -->
            <mass value="0.564" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.003881243" ixy="0.0" ixz="0.0" iyy="0.000498940" iyz="0.0" izz="0.003879257" />
        </inertial>
    </link>
    <!-- camera depth joints and links -->
    <joint name="camera_rgbd_camera_depth_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.0 -0.0115 0.0" />
        <parent link="camera_rgbd_camera_link" />
        <child link="camera_rgbd_camera_depth_frame" />
    </joint>
    <link name="camera_rgbd_camera_depth_frame" />
    <joint name="camera_rgbd_camera_depth_optical_joint" type="fixed">
        <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0" />
        <parent link="camera_rgbd_camera_depth_frame" />
        <child link="camera_rgbd_camera_depth_optical_frame" />
    </joint>
    <link name="camera_rgbd_camera_depth_optical_frame" />
    <!-- camera left IR joints and links -->
    <joint name="camera_rgbd_camera_left_ir_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 -0.021 0" />
        <parent link="camera_rgbd_camera_depth_frame" />
        <child link="camera_rgbd_camera_left_ir_frame" />
    </joint>
    <link name="camera_rgbd_camera_left_ir_frame" />
    <joint name="camera_rgbd_camera_left_ir_optical_joint" type="fixed">
        <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0" />
        <parent link="camera_rgbd_camera_left_ir_frame" />
        <child link="camera_rgbd_camera_left_ir_optical_frame" />
    </joint>
    <link name="camera_rgbd_camera_left_ir_optical_frame" />
    <!-- camera right IR joints and links -->
    <joint name="camera_rgbd_camera_right_ir_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0.029 0" />
        <parent link="camera_rgbd_camera_depth_frame" />
        <child link="camera_rgbd_camera_right_ir_frame" />
    </joint>
    <link name="camera_rgbd_camera_right_ir_frame" />
    <joint name="camera_rgbd_camera_right_ir_optical_joint" type="fixed">
        <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0" />
        <parent link="camera_rgbd_camera_right_ir_frame" />
        <child link="camera_rgbd_camera_right_ir_optical_frame" />
    </joint>
    <link name="camera_rgbd_camera_right_ir_optical_frame" />
    <!-- camera fisheye joints and links -->
    <joint name="camera_rgbd_camera_fisheye_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0.044 0" />
        <parent link="camera_rgbd_camera_depth_frame" />
        <child link="camera_rgbd_camera_fisheye_frame" />
    </joint>
    <link name="camera_rgbd_camera_fisheye_frame" />
    <joint name="camera_rgbd_camera_fisheye_optical_joint" type="fixed">
        <origin rpy="-1.5707963267948966 0 -1.5707963267948966" xyz="0 0 0" />
        <parent link="camera_rgbd_camera_fisheye_frame" />
        <child link="camera_rgbd_camera_fisheye_optical_frame" />
    </joint>
    <link name="camera_rgbd_camera_fisheye_optical_frame" />
    <!-- gazebo reference="${prefix}_rgb_base_link" -->
    <gazebo reference="camera_rgbd_camera_link">
        <sensor name="camera_rgbd_depth_sensor" type="depth">
            <always_on>true</always_on>
            <update_rate>15.0</update_rate>
            <camera>
                <horizontal_fov>1.5917402778188288</horizontal_fov>
                <image>
                    <format>R8G8B8</format>
                    <width>640</width>
                    <height>480</height>
                </image>
                <clip>
                    <near>0.3</near>
                    <far>10.0</far>
                </clip>
            </camera>
            <plugin filename="libgazebo_ros_openni_kinect.so" name="camera_rgbd_controller">
                <cameraName>camera_rgbd</cameraName>
                <alwaysOn>true</alwaysOn>
                <updateRate>10</updateRate>
                <imageTopicName>rgb/image_raw</imageTopicName>
                <depthImageTopicName>depth/image_raw</depthImageTopicName>
                <pointCloudTopicName>depth/points</pointCloudTopicName>
                <cameraInfoTopicName>rgb/camera_info</cameraInfoTopicName>
                <depthImageCameraInfoTopicName>depth/camera_info</depthImageCameraInfoTopicName>
                <frameName>/camera_rgbd_camera_depth_optical_frame</frameName>
                <baseline>0.1</baseline>
                <distortion_k1>0.0</distortion_k1>
                <distortion_k2>0.0</distortion_k2>
                <distortion_k3>0.0</distortion_k3>
                <distortion_t1>0.0</distortion_t1>
                <distortion_t2>0.0</distortion_t2>
                <pointCloudCutoff>0.4</pointCloudCutoff>
            </plugin>
        </sensor>
    </gazebo>
    <!-- Kinectv2 -->
    <!--xacro:sensor_kinectv2 prefix="camera_rgbd" parent="world"-->
    <!--/xacro:sensor_kinectv2-->
    <transmission name="trans_egh_gripper_finger_right_joint">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="egh_gripper_finger_right_joint">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="egh_gripper_finger_right_joint_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>
</robot>

